					wait 1 tick
					send "<##FE8120>🔮 <##B2FFF6>Charging &f🧨&6fireball"
					execute console command "execute as %player% at @s run summon armor_stand ~ ~ ~ {Tags:[""fire%player%""],Invisible:1b,Marker:1b}"
					execute console command "execute as %player% at @s run item replace entity @e[type=armor_stand,distance=0..2] armor.head with fire_charge"
					make last spawned entity ride player


					
					set {Fireball.%{_uuid}%} to 1
					set {ShotFireball.%{_uuid}%} to true
					set {ShootingFireball.%{_uuid}%} to true
					set {_bar} to boss bar with id "fire %{_uuid}%" with title "Fireball Power" with color red with progress 10
