			if {Fire.Power3.Campfire.Second.%{_uuid}%} is not set:
				set {Fire.Power3.Campfire.Second.%{_uuid}%} to 0
				set {Fire.Power3.Campfire.Minute.%{_uuid}%} to 0
				set {Fire.Power3.Campfire.Cooldown.%{_uuid}%} to true
		
			if {Fire.Power3.Campfire.Cooldown.%{_uuid}%} is true:
				if {Fire.Power3.Campfire.Minute.%{_uuid}%} is 0:
					set {Fire.Power3.Campfire.Display.%{_uuid}%} to "&f🥾 &b%{Fire.Power3.Campfire.Second.%{_uuid}%}%s"

				else:
					set {Fire.Power3.Campfire.Display.%{_uuid}%} to "&f🥾 &b%{Fire.Power3.Campfire.Minute.%{_uuid}%}%m %{Fire.Power3.Campfire.Second.%{_uuid}%}%s"

