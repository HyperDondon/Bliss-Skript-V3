command /testfire:
	permission: op
	trigger:
		set {_loc} to the location 0.4 meters under player's head
		
		set {_yaw} to yaw of player
		set {_pitch} to pitch of player
		
		spawn 1 fireball at {_loc} with nbt compound from "{ExplosionPower:10, Motion:[%{_yaw}%,%{_pitch}%,0.0]}"

 


	
