							set {Life.Power3.CircleOfLfe.Display.%{_uuid}%} to "💖 &b%{Life.Power3.CircleOfLfe.Minute.%{_uuid}%}%m %{Life.Power3.CircleOfLfe.Second.%{_uuid}%}%s"		
					if {Life.Power3.CircleOfLfe.Second.%{_uuid}%} is not set:
						set {Life.Power3.CircleOfLfe.Second.%{_uuid}%} to 0
						set {Life.Power3.CircleOfLfe.Minute.%{_uuid}%} to 0
						set {Life.Power3.CircleOfLfe.Cooldown.%{_uuid}%} to true

					if {Life.Power3.CircleOfLfe.Display.%{_uuid}%} is not set:
						set {Life.Power3.CircleOfLfe.Display.%{_uuid}%} to "💖 &b%{Life.Power3.CircleOfLfe.Minute.%{_uuid}%}%m %{Life.Power3.CircleOfLfe.Second.%{_uuid}%}%s"
						
				
					if {Life.Power3.CircleOfLfe.Cooldown.%{_uuid}%} is true:
						if {Life.Power3.CircleOfLfe.Minute.%{_uuid}%} is 0:
							set {Life.Power3.CircleOfLfe.Display.%{_uuid}%} to "💖 &b%{Life.Power3.CircleOfLfe.Second.%{_uuid}%}%s"

						else:
							set {Life.Power3.CircleOfLfe.Display.%{_uuid}%} to "💖 &b%{Life.Power3.CircleOfLfe.Minute.%{_uuid}%}%m %{Life.Power3.CircleOfLfe.Second.%{_uuid}%}%s"

					if {Life.Power3.CircleOfLfe.Cooldown.%{_uuid}%} is true:
						remove 1 from {Life.Power3.CircleOfLfe.Second.%{_uuid}%}



					if {Life.Power3.CircleOfLfe.Second.%{_uuid}%} < 0:
						remove 1 from {Life.Power3.CircleOfLfe.Minute.%{_uuid}%}
						set {Life.Power3.CircleOfLfe.Second.%{_uuid}%} to 59
						
					if {Life.Power3.CircleOfLfe.Minute.%{_uuid}%} < 0:
						set {Life.Power3.CircleOfLfe.Cooldown.%{_uuid}%} to false
						set {Life.Power3.CircleOfLfe.Display.%{_uuid}%} to "💖 &aReady!"


					#Healer

					if {Life.Power2.Healer.Second.%{_uuid}%} is not set:
						set {Life.Power2.Healer.Second.%{_uuid}%} to 0
						set {Life.Power2.Healer.Minute.%{_uuid}%} to 0
						set {Life.Power2.Healer.Cooldown.%{_uuid}%} to true

					if {Life.Power2.Healer.Minute.%{_uuid}%} is not set:
						set {Life.Power2.Healer.Second.%{_uuid}%} to 0
						set {Life.Power2.Healer.Minute.%{_uuid}%} to 0
						set {Life.Power2.Healer.Cooldown.%{_uuid}%} to true


					if {Life.Power2.Healer.Cooldown.%{_uuid}%} is not set:
						set {Life.Power2.Healer.Second.%{_uuid}%} to 0
						set {Life.Power2.Healer.Minute.%{_uuid}%} to 0
						set {Life.Power2.Healer.Cooldown.%{_uuid}%} to true	
