function LifeLine(p1: entity, p2: entity):
	if {_p2} is a player:
		set {_v} to vector from {_p1}'s head to {_p2}'s head
		set vector length of {_v} to 0.1
		set {_loc} to the location 0.7 meters under {_p1}'s head
		loop (distance between {_loc} and {_p2}) / 0.1 times:

			draw 1 dust using dustOption(rgb(254, 4, 180), 1) at {_loc} with force
			draw 1 dust_color_transition using dustTransition(black, black, 1) at {_loc} with force
			set {_loc} to {_loc} ~ {_v}	

	else:
		if {_p2} is alive:
			set {_v} to vector from {_p1}'s head to {_p2}'s head
			set vector length of {_v} to 0.1
			set {_loc} to the location 0.7 meters under {_p1}'s head
			loop (distance between {_loc} and {_p2}) / 0.1 times:

				draw 1 dust using dustOption(rgb(254, 4, 180), 1) at {_loc} with force
				draw 1 dust_color_transition using dustTransition(black, black, 1) at {_loc} with force
				set {_loc} to {_loc} ~ {_v}	






