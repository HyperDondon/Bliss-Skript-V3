command /trust [<text>] [<offlineplayer>] [<integer>]:
	trigger:
		if arg-1 is "add":
			if arg-2 is set:
				if arg-3 is 1:
					set {Trusted.%player%.1} to arg-2
					send "&c%player% has added you as &6Trusted player" to arg-2
					execute console command "tag %arg-2% add TrustedBy%player%"
				if arg-3 is 2:
					set {Trusted.%player%.2} to arg-2
					send "&c%player% has added you as &6Trusted player" to arg-2
					execute console command "tag %arg-2% add TrustedBy%player%"
				if arg-3 is 3:
					set {Trusted.%player%.3} to arg-2
					send "&c%player% has added you as &6Trusted player" to arg-2
					execute console command "tag %arg-2% add TrustedBy%player%"
		if arg-1 is "remove":
			if arg-2 is set:
				if arg-3 is 1:
					set {Trusted.%player%.1} to false
					send "&c%arg-1% is removed from trust list" to arg-2
					execute console command "tag %arg-2% remove TrustedBy%player%"
				if arg-3 is 2:
					set {Trusted.%player%.1} to false
					send "&c%arg-1% is removed from trust list" to arg-2
					execute console command "tag %arg-2% remove TrustedBy%player%"
				if arg-3 is 3:
					set {Trusted.%player%.1} to false
					send "&c%arg-1% is removed from trust list" to arg-2
					execute console command "tag %arg-2% remove TrustedBy%player%"
				
on tab complete for "/trust":
    set tab completions for position 1 to "add" and "remove"
		


