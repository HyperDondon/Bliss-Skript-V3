						set {Life.Power2.Healer.Minute.%{_uuid}%} to 0
						set {Life.Power2.Healer.Cooldown.%{_uuid}%} to true	

					remove 1 from {Life.Power2.Healer.Second.%{_uuid}%}
					set {Life.Power2.Healer.Display.%{_uuid}%} to " &b%{Life.Power2.Healer.Second.%{_uuid}%}%s"

					if {Life.Power2.Healer.Second.%{_uuid}%} < 1:
						set {Life.Power2.Healer.Second.%{_uuid}%} to 6
						heallife(loop-player)

					#HeartDrainer





					if {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} is true:
						remove 1 from {Life.Power1.HeartDrainer.Second.%{_uuid}%}


					
					if {Life.Power1.HeartDrainer.Second.%{_uuid}%} < 0:
						remove 1 from {Life.Power1.HeartDrainer.Minute.%{_uuid}%}
						set {Life.Power1.HeartDrainer.Second.%{_uuid}%} to 59


					if {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} is true:
						if {Life.Power1.HeartDrainer.Minute.%{_uuid}%} is 0:
							set {Life.Power1.HeartDrainer.Display.%{_uuid}%} to "💘&b%{Life.Power1.HeartDrainer.Second.%{_uuid}%}%s"

						else:
							set {Life.Power1.HeartDrainer.Display.%{_uuid}%} to "💘&b%{Life.Power1.HeartDrainer.Minute.%{_uuid}%}%m %{Life.Power1.HeartDrainer.Second.%{_uuid}%}%s"


					if {Life.Power1.HeartDrainer.Second.%{_uuid}%} is not set:
						set {Life.Power1.HeartDrainer.Second.%{_uuid}%} to 0
						set {Life.Power1.HeartDrainer.Minute.%{_uuid}%} to 0

					if {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} is not set:
						set {Life.Power1.HeartDrainer.Second.%{_uuid}%} to 0
						set {Life.Power1.HeartDrainer.Minute.%{_uuid}%} to 0
						set {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} to true	

					if {Life.Power1.HeartDrainer.Display.%{_uuid}%} is not set:
						set {Life.Power1.HeartDrainer.Second.%{_uuid}%} to 0
						set {Life.Power1.HeartDrainer.Minute.%{_uuid}%} to 0
						set {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} to true	


					if {Life.Power1.HeartDrainer.Display.%{_uuid}%} is not set:
						set {Life.Power1.HeartDrainer.Display.%{_uuid}%} to "💘&b%{Life.Power1.HeartDrainer.Minute.%{_uuid}%}%m %{Life.Power1.HeartDrainer.Second.%{_uuid}%}%s"



					if {Life.Power1.HeartDrainer.Minute.%{_uuid}%} < 0:
						set {Life.Power1.HeartDrainer.Cooldown.%{_uuid}%} to false
