				set {Puff.Power1.Dash.Second.%{_uuid}%} to 59


			if {Puff.Power1.Dash.Cooldown.%{_uuid}%} is true:
				if {Puff.Power1.Dash.Minute.%{_uuid}%} is 0:
					set {Puff.Power1.Dash.Display.%{_uuid}%} to "&f‚è´ &b%{Puff.Power1.Dash.Second.%{_uuid}%}%s"

				else:
					set {Puff.Power1.Dash.Display.%{_uuid}%} to "&f‚è´ &b%{Puff.Power1.Dash.Minute.%{_uuid}%}%m %{Puff.Power1.Dash.Second.%{_uuid}%}%s"


