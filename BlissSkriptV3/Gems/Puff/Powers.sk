			if {Puff.Power2.DoubleJump.Second.%{_uuid}%} is -1:
				remove 1 from {Puff.Power2.DoubleJump.Minute.%{_uuid}%}
				set {Puff.Power2.DoubleJump.Second.%{_uuid}%} to 59
                
			if {Puff.Power2.DoubleJump.Minute.%{_uuid}%} is -1:
				set {Puff.Power2.DoubleJump.Cooldown.%{_uuid}%} to false
				set {Puff.Power2.DoubleJump.Display.%{_uuid}%} to " &aReady!"



			#Dash





			if {Puff.Power1.Dash.Cooldown.%{_uuid}%} is true:
